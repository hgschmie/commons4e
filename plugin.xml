<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.jiayun.commons4e"
   name="Commons4e Plug-in"
   version="1.0.0"
   provider-name="JiaYun"
   class="org.jiayun.commons4e.Commons4ePlugin">

   <runtime>
      <library name="commons4e.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jdt.ui"/>
   </requires>
   <extension
         point="org.eclipse.ui.popupMenus">
      <viewerContribution
            targetID="#CompilationUnitEditorContext"
            id="org.jiayun.commons4e.lang.LangActions">
         <menu
               label="Commons4e Lang"
               path="additions"
               id="langMenu">
            <separator name="langGroup"/>
         </menu>
         <action
               label="Generate Equals Method..."
               class="org.jiayun.commons4e.lang.actions.GenerateEqualsAction"
               menubarPath="langMenu/langGroup"
               id="org.jiayun.commons4e.lang.actions.GenerateEqualsAction"/>
      </viewerContribution>
   </extension>

</plugin>
